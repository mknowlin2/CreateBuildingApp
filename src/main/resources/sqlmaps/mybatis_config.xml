  <mappers>
    <mapper resource="com/webapp/knowlin/persistence/BuildingMapper.xml"/>
    <mapper resource="com/webapp/knowlin/persistence/AddressMapper.xml"/>
    <mapper resource="com/webapp/knowlin/persistence/FloorMapper.xml"/>
  </mappers>
</configuration>